# Specify the BigQuery table in the format 'project.dataset.table'
table_name = "vz-it-pr-fjpv-vzdsdo-0.ds_work_tbls.condition_lis_train_gcp"

# Read the table into a PySpark DataFrame
train_data = spark.read \
    .format("bigquery") \
    .option("table", table_name) \
    .load()

# Show the DataFrame to verify the results
train_data.show()
